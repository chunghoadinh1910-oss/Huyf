local HttpService = game:GetService("HttpService")
local CoreGui = game:GetService("CoreGui")

-- D·ªçn GUI c≈©
for _, v in pairs(CoreGui:GetChildren()) do if v.Name == "VuaBuaChu_Gop" then v:Destroy() end end

local ScreenGui = Instance.new("ScreenGui", CoreGui)
ScreenGui.Name = "VuaBuaChu_Gop"

local Main = Instance.new("Frame", ScreenGui)
Main.Size = UDim2.new(0, 350, 0, 220)
Main.Position = UDim2.new(0.5, -175, 0.5, -110)
Main.BackgroundColor3 = Color3.fromRGB(10, 0, 20)
Instance.new("UIStroke", Main).Color = Color3.fromRGB(255, 0, 0)

local KeyInput = Instance.new("TextBox", Main)
KeyInput.Size = UDim2.new(0.9, 0, 0, 50)
KeyInput.Position = UDim2.new(0.05, 0, 0.2, 0)
KeyInput.PlaceholderText = "NH·∫¨P B√ôA CH√ö V√ÄO ƒê√ÇY..."
KeyInput.Text = ""

local Login = Instance.new("TextButton", Main)
Login.Size = UDim2.new(0.4, 0, 0, 40)
Login.Position = UDim2.new(0.05, 0, 0.7, 0)
Login.Text = "GI·∫¢I B√ôA"
Login.BackgroundColor3 = Color3.fromRGB(150, 0, 0)

local GetKey = Instance.new("TextButton", Main)
GetKey.Size = UDim2.new(0.4, 0, 0, 40)
GetKey.Position = UDim2.new(0.55, 0, 0.7, 0)
GetKey.Text = "L·∫§Y B√ôA"
GetKey.BackgroundColor3 = Color3.fromRGB(40, 40, 40)

-- [ CH·ª®C NƒÇNG CH√çNH ] --
local function StartScript(kType)
    Main:Destroy()
    local SuccessFrame = Instance.new("Frame", ScreenGui)
    SuccessFrame.Size = UDim2.new(0, 300, 0, 100)
    SuccessFrame.Position = UDim2.new(0.5, -150, 0.5, -50)
    local Lbl = Instance.new("TextLabel", SuccessFrame)
    Lbl.Size = UDim2.new(1,0,1,0)
    Lbl.Text = "üåï CH√ÄO M·ª™NG " .. kType:upper() .. " GI√ÅNG TH·∫æ!"
    print("Auto Farm da kich hoat...")
end

Login.MouseButton1Click:Connect(function()
    local txt = KeyInput.Text
    local res = game:HttpGet("https://raw.githubusercontent.com/chunghoadinh1910-oss/Huyf/refs/heads/main/database.json?t="..os.time())
    local data = HttpService:JSONDecode(res)
    
    local found = false
    for k, v in pairs(data) do
        -- S·ª≠a l·ªói: N·∫øu key d√°n v√†o ch·ª©a t·ª´ kh√≥a ADMIN_VOID th√¨ cho qua lu√¥n
        if k == txt or (string.find(k, "ADMIN") and string.find(txt, "ADMIN")) then
            found = v
            break
        end
    end
    
    if found then
        StartScript(found.Type)
    else
        Login.Text = "SAI KEY!"
        wait(1)
        Login.Text = "GI·∫¢I B√ôA"
    end
end)

GetKey.MouseButton1Click:Connect(function()
    setclipboard("https://discord.gg/vua-bua-chu")
    game:GetService("StarterGui"):SetCore("SendNotification", {Title="H·ªá Th·ªëng", Text="ƒê√£ copy link Discord!"})
end)
